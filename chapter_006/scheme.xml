<?xml version="1.0"?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
	<xsl:template match="/">
		<xsl:text/>
        	<entries>
			<xsl:text/>
                	<xsl:for-each select="entries/values">
                		<entry>
                			<xsl:attribute name="field">
                                		<xsl:value-of select="value"/>
                           		</xsl:attribute>
                      		</entry>
                	</xsl:for-each>
                </entries>
   	</xsl:template>
</xsl:stylesheet>